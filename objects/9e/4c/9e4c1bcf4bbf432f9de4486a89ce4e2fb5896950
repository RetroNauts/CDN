# Configuration file

####################
# "!enable"
####################

""!enable"" {
    B:"Enable Utility Set"=true
    ####################
    # "enable metals"
    ####################

    ""enable metals"" {
        B:"Enable Bitumen"=true
        B:"Enable Magnesium"=true
        B:"Enable Phosphorite"=true
        B:"Enable Potash"=true
        B:"Enable Saltpeter"=true
        B:"Enable Sulfur"=true
    }

    ####################
    # enable metals
    ####################

    "enable metals" {
        B:"Enable Bitumen"=true
        B:"Enable Magnesium"=true
        B:"Enable Phosphorite"=true
        B:"Enable Potash"=true
        B:"Enable Saltpeter"=true
        B:"Enable Sulfur"=true
    }

}


####################
# 1_enable
####################

1_enable {
    B:enable_utility_set=true
    ####################
    # enable_metals
    ####################

    enable_metals {
        B:bitumen=true
        B:magnesium=true
        B:phosphorite=true
        B:potash=true
        B:saltpeter=true
        B:sulfur=true
    }

    ####################
    # enable_world_gen
    ####################

    enable_world_gen {
        B:bitumen=true
        B:magnesium=true
        B:phosphorite=true
        B:potash=true
        B:saltpeter=true
        B:sulfur=true
    }

}


####################
# bitumen
####################

bitumen {
    ####################
    # misc
    ####################

    misc {
        I:"Block Hardness Level"=1
        I:block_hardness_level=1
    }

    ####################
    # "world gen"
    ####################

    ""world gen"" {
        S:Diminsions=0 2-100000
        I:"Max Height"=128
        I:"Min Height"=0
        I:"Vein Chance"=100
        I:"Vein Density"=100
        I:"Vein Size"=4
        I:"Veins Per Chunk"=4
    }

    ####################
    # world_gen
    ####################

    world_gen {
        S:diminsions=0 2-100000
        I:max_height=128
        I:min_height=0
        I:vein_chance=100
        I:vein_density=100
        I:vein_size=4
        I:veins_per_chunk=4
    }

}


####################
# block
####################

block {
    I:Ore=915
    I:he_tnt=930
    I:le_tnt=931
    I:ore=910
}


