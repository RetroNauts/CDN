# Configuration file

####################
# """""!enable"""""
####################

""""""!enable"""""" {
    B:"Enable Precious Set"=true
    ####################
    # """""enable metals"""""
    ####################

    """"""enable metals"""""" {
        B:"Enable Brass"=true
        B:"Enable Electrum"=true
        B:"Enable Platinum"=true
        B:"Enable Silver"=true
        B:"Enable Zinc"=true
    }

}


####################
# 1_enable
####################

1_enable {
    B:enable_precious_set=true
    ####################
    # enable_metals
    ####################

    enable_metals {
        B:brass=true
        B:electrum=true
        B:platinum=true
        B:silver=true
        B:zinc=true
    }

    ####################
    # tool_recipes
    ####################

    tool_recipes {
        B:brass=true
        B:electrum=true
        B:platinum=true
        B:silver=true
    }

    ####################
    # armor_recipes
    ####################

    armor_recipes {
        B:brass=true
        B:electrum=true
        B:platinum=true
        B:silver=true
    }

    ####################
    # enable_world_gen
    ####################

    enable_world_gen {
        B:platinum=true
        B:silver=true
        B:zinc=true
    }

}


####################
# block
####################

block {
    I:Block=912
    I:Brick=913
    I:Ore=914
    I:block=905
    I:brick=906
    I:ore=904
}


