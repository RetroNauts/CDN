# Configuration file

####################
# """"""""!enable""""""""
####################

"""""""""!enable""""""""" {
    B:"Enable Base Set"=true
    ####################
    # """"""""enable metals""""""""
    ####################

    """""""""enable metals""""""""" {
        B:"Enable Angmallen"=true
        B:"Enable Bronze"=true
        B:"Enable Copper"=true
        B:"Enable Damascus Steel"=true
        B:"Enable Hepatizon"=true
        B:"Enable Manganese"=true
        B:"Enable Steel"=true
        B:"Enable Tin"=true
    }

}


####################
# 1_enable
####################

1_enable {
    B:enable_base_set=true
    ####################
    # enable_metals
    ####################

    enable_metals {
        B:angmallen=true
        B:bronze=true
        B:copper=true
        B:damascus_steel=true
        B:hepatizon=true
        B:manganese=true
        B:steel=true
        B:tin=true
    }

    ####################
    # tool_recipes
    ####################

    tool_recipes {
        B:angmallen=true
        B:bronze=true
        B:copper=true
        B:damascus_steel=true
        B:hepatizon=true
        B:steel=true
    }

    ####################
    # armor_recipes
    ####################

    armor_recipes {
        B:angmallen=true
        B:bronze=true
        B:copper=true
        B:damascus_steel=true
        B:hepatizon=true
        B:steel=true
    }

    ####################
    # enable_world_gen
    ####################

    enable_world_gen {
        B:copper=true
        B:manganese=true
        B:tin=true
    }

}


####################
# angmallen
####################

angmallen {
    ####################
    # misc
    ####################

    misc {
        I:"Block Hardness Level"=2
        I:"abstractor xp"=9
        I:abstractor_xp=9
        I:block_hardness_level=2
    }

    ####################
    # """"""""tool info""""""""
    ####################

    """""""""tool info""""""""" {
        I:Damage=2
        I:Durability=300
        I:Enchantability=30
        I:"Pick Level"=2
        I:Speed=10
    }

    ####################
