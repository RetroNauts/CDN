}


####################
# 1_enable
####################

1_enable {
    B:enable_nether_set=true
    ####################
    # item_recipes
    ####################

    item_recipes {
        B:midasium_to_gold=true
    }

    ####################
    # enable_metals
    ####################

    enable_metals {
        B:alduorite=true
        B:amordrine=true
